{
  "name": "url-opener",
  "displayName": "URL Opener",
  "description": "Open a URL or keyword!",
  "publisher": "Young-Vform",
  "repository": {
    "url": "https://github.com/youngvform/vscode-url-opener"
  },
  "icon": "images/logo.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:url-opener.openUrl",
    "onCommand:url-opener.openSelectedUrl",
    "onCommand:url-opener.searchKeyword",
    "onCommand:url-opener.searchSelectedKeyword"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "url-opener.openUrl",
        "title": "Open a url",
        "category": "URL Opener"
      },
      {
        "command": "url-opener.openSelectedUrl",
        "title": "Open a selected url",
        "category": "URL Opener"
      },
      {
        "command": "url-opener.searchKeyword",
        "title": "Search keywords",
        "category": "URL Opener"
      },
      {
        "command": "url-opener.searchSelectedKeyword",
        "title": "Search selected keywords",
        "category": "URL Opener"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "URL Opener",
      "properties": {
        "url-opener.defaultSearchUrl": {
          "type": "string",
          "default": "https://www.google.com/search?q=",
          "description": "Set custom search engine url (default is google)"
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "url-opener.openSelectedUrl",
          "when": "false"
        },
        {
          "command": "url-opener.searchSelectedKeyword",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "url-opener.openSelectedUrl",
          "group": "url-opener"
        },
        {
          "when": "editorHasSelection",
          "command": "url-opener.searchSelectedKeyword",
          "group": "url-opener"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "opn": "^6.0.0"
  }
}
